result=$(rsync -e 'ssh' -avz --exclude 'tags' --exclude '.DS_Store' --exclude '.FBCLockFolder' --exclude 'templates_c/' --exclude 'css/compiled/' --exclude 'app/compiled/smarty/' --exclude '*.git' --exclude '*.out' --exclude '*.mlog' --delete $1 $2)
echo $result
terminal-notifier -message "$result"
